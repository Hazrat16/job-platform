# Local Development Environment (without Docker)
# Copy this to .env.local and modify as needed

NODE_ENV=development
PORT=5000
JWT_SECRET=dev_jwt_secret_key_change_in_production

# Use local MongoDB if available, or Docker ports
MONGODB_URI=mongodb://localhost:27018/job-platform
RABBITMQ_URL=amqp://localhost:5673
REDIS_URL=redis://localhost:6380

# Client URL
CLIENT_URL=http://localhost:3000

# Debug
DEBUG=true
LOG_LEVEL=debug
